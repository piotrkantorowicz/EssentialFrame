<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>opencover</Format>
          <Exclude>
            [EssentialFrame.Cqrs.Commands.Tests.UnitTests]*,
            [EssentialFrame.Core.Tests.UnitTests]*,
            [EssentialFrame.Domain.Tests.UnitTests]*,
            [EssentialFrame.Domain.EventSourcing.Tests.UnitTests]*,
            [EssentialFrame.Cqrs.Queries.Tests.UnitTests]*,
            [EssentialFrame.Tests.ArchitectureTests]*,
            [EssentialFrame.Tests.IntegrationTests]*,
            [EssentialFrame.Tests.Utils]*,
            [EssentialFrame.ExampleApp]*
          </Exclude>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>